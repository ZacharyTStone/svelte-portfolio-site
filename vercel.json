{
	"headers": [
		{
			"source": "/(.*)\\.(js|css|png|jpg|jpeg|webp|svg|ico|woff2?)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, s-maxage=604800, stale-while-revalidate=2592000"
				}
			]
		}
	]
}


